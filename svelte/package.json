{
	"name": "notidian",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"dev:electron": "electron electron/electron.cjs",
		"build": "cross-env NODE_ENV=production pnpm build:svelte && pnpm build:electron",
		"build:svelte": "vite build",
		"build:electron": "electron-builder -mwl --config build.config.json",
		"prepare": "svelte-kit sync",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint --ignore-path .gitignore .",
		"tauri": "tauri",
		"release": "vite build && tauri build",
		"package": "pnpm run build && electron-builder --config electron-builder.config.json"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "1.0.0-next.36",
		"@sveltejs/kit": "next",
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/typography": "^0.5.4",
		"@tauri-apps/cli": "^1.0.3",
		"@tiptap/core": "2.0.0-beta.182",
		"@typescript-eslint/eslint-plugin": "^5.30.6",
		"@typescript-eslint/parser": "^5.30.6",
		"autoprefixer": "^10.4.7",
		"concurrently": "^7.2.2",
		"cross-env": "^7.0.3",
		"cssnano": "^5.1.12",
		"electron": "^19.0.8",
		"electron-builder": "^23.1.0",
		"electron-reloader": "^1.2.3",
		"eslint": "^8.19.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.14",
		"postcss-font-magician": "^3.0.0",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.6",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vite": "^3.0.0"
	},
	"type": "module",
	"dependencies": {
		"@tailwindcss/line-clamp": "^0.4.0",
		"@tauri-apps/api": "^1.0.2",
		"@tiptap/extension-bubble-menu": "2.0.0-beta.61",
		"@tiptap/extension-code-block-lowlight": "2.0.0-beta.73",
		"@tiptap/extension-dropcursor": "2.0.0-beta.29",
		"@tiptap/extension-highlight": "2.0.0-beta.35",
		"@tiptap/extension-image": "2.0.0-beta.30",
		"@tiptap/extension-link": "2.0.0-beta.43",
		"@tiptap/extension-placeholder": "2.0.0-beta.53",
		"@tiptap/extension-task-item": "2.0.0-beta.37",
		"@tiptap/extension-task-list": "2.0.0-beta.29",
		"@tiptap/extension-underline": "2.0.0-beta.25",
		"@tiptap/extension-youtube": "2.0.0-beta.6",
		"@tiptap/starter-kit": "2.0.0-beta.191",
		"@tiptap/suggestion": "2.0.0-beta.97",
		"chokidar": "^3.5.3",
